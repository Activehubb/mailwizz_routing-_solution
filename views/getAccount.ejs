  <body>
    <!--====== SIGNIN ONE PART START ======-->

    <section class="signin-area signin-one">
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-lg-5 my-4">
            <form action="/onboarding/account" method="post">
              <div class="signin-form form-style-two rounded-buttons">
                <div class="row">
                  <div class="col-md-12">
                    <div class="form-input">
                      <label>Your APIKEY accountPublicKey</label>
                      <div class="input-items default">
                        <input
                          type="text"
                          placeholder="Account public key"
                          name="accountPublicKey"
                        />
                        <i class="lni lni-invention"></i>
                      </div>
                    </div>
                    <!-- form input -->
                  </div>
                  <div class="col-md-12">
                    <div class="form-input">
                      <label>Enter your APIKEY</label>
                      <div class="input-items default">
                        <input
                          type="text"
                          placeholder="API KEY"
                          name="accountAPIkey"
                        />
                        <i class="lni lni-key"></i>
                      </div>
                    </div>
                    <!-- form input -->
                  </div>
                  <div class="col-md-12">
                    <div class="form-input">
                      <label>Your APIKEY baseURL</label>
                      <div class="input-items default">
                        <input
                          type="text"
                          placeholder="Your APIKEY baseURL"
                          name="baseURL"
                        />
                        <i class="lni lni-information"></i>
                      </div>
                    </div>
                    <!-- form input -->
                  </div>
                  <div class="col-md-12">
                    <div class="form-input rounded-buttons">
                      <button
                        class="btn primary-btn btn-rounded-full"
                        type="submit"
                      >
                        Create Account
                      </button>
                    </div>
                    <!-- form input -->
                  </div>
                </div>
              </div>
              <!-- signin form -->
            </form>
          </div>
        </div>
